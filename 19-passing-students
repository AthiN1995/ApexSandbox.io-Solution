public Boolean isPassed(Boolean passedExam, Boolean passedAssignments, Boolean passedProject) {
    Boolean b;
    if(passedExam==true){
        if(passedAssignments==true){
             b=true;
        } else b=false;
    } else if(passedExam==true){
        if(passedProject==true){
             b=true;
        } else b=false;
    } else if(passedProject==true){
        if(passedAssignments==true){
             b=true;
        } else b=false;
    }
    return b;
}