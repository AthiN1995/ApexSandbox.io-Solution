public Boolean isLeapYear(Integer year) {
    Integer x=math.mod(year,4);
    Integer y=math.mod(year,100);
    Integer z=math.mod(year,400);
    boolean res=false;
    if(x==0){
        if((y==0)&&(z==0)){
            res=true;
        }
    } else if((x==0)&&(y!=0)){
        res=true;
    }
    return res;
}
