public String aOrAn(String word) {
    if(word.startswith('a')){
        string text = 'an '+word;
        return text;
    }
    else if(word.startswith('e')){
        string text = 'an '+word;
        return text;
    } 
    else if(word.startswith('i')){
        string text = 'an '+word;
        return text;
    }
    else if(word.startswith('o')){
        string text = 'an '+word;
        return text;
    }
    else if(word.startswith('u')){
        string text = 'an '+word;
        return text;
    }
    else {
        string text = 'a '+word;
        return text;
    }
}