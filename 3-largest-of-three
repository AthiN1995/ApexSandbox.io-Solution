public static Integer findLargest(Integer num1, Integer num2, Integer num3) {
	integer big;
    if(num1>=num2){
        if(num1>=num3){
            big=num1;
        }
    } else if(num3>=num2){
        if(num3>=num1){
            big=num3;
        }
    } else {
        big=num2;
    }
    return big;
}